<%- include('z01header') %>

<div class="jumbotron">

    <h1><%= title %></h1>

    <p class="lead">
        Dead simple, slightly opinionated Express.js demo. Also see
        <a href="./hello">JSON response</a> and
        <a href="./gone">page not found</a> examples.
    </p>

</div> <!-- class jumbotron -->

<div class="row">

    <div class="col-md-6">

        <% if(typeof ejsObj.directoryData !== 'undefined' &&
             ejsObj.directoryData.length > 0) { %>

            <h2>Descriptions of project files</h2>

            <table class="table table-striped table-hover">
                <tbody>

                <% ejsObj.directoryData.forEach(function(item) { %>

                    <tr>
                        <td><%= item.name %></td>
                        <td><%= item.desc %></td>
                    </tr>

                <% }); %>

                </tbody>
            </table>

        <% } %>

    </div>

    <div class="col-md-6">

        <h2>Supported cipher algorithms</h2>

        <pre>
<%= JSON.stringify(ejsObj.supportedCiphers, null, 2) %>
        </pre>

    </div>

</div>

<%- include('z02footer') %>
